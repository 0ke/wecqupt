/**about.wxss**/
.container{
    background: #f4f4f4;
    padding: 0;
}
.header{
    width: 100%;
    height: 455rpx;
    padding-top: 110rpx;
    text-align: center;
    box-sizing: border-box;
}
.logo{
    display: inline-block;
    width: 277rpx;
    height: 277rpx;
    border-radius: 50%;
}
.content{
    flex: 1;
}
.version{
    height: 104rpx;
    background: #fff;
    display: flex;
    justify-content: space-between;
    padding: 0 55rpx;
    line-height: 104rpx;
    box-sizing: border-box;
    font-size: 12pt;
    color: #666;
    font-weight: 200;
    border-top: 1px #e5e5e5 solid;
    border-bottom: 1px #e5e5e5 solid;
}
.navigator{
    color: #339c74;
}
.describe{
    width: 100%;
    box-sizing: border-box;
    padding: 10rpx 55rpx 20rpx;
    display: flex;
    flex-flow: column nowrap;
    color: #545454;
    line-height: 2;
    background: #fff;
    margin-top: 12rpx;
    border-top: 1px #e5e5e5 solid;
    border-bottom: 1px #e5e5e5 solid;
}
.desc-title{
    font-size: 12pt;
    line-height: 2.5;
}
.footer{
    text-align: center;
    padding: 40rpx 0 20rpx;
    color: #ccc;
}
.footer-lanshan{
    height: 32rpx;
    line-height: 32rpx;
    display: flex;
    align-items: stretch;
    justify-content: center;
}
.footer-lanshan image{
    width: 32rpx;
    height: 32rpx;
}
