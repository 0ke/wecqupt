/**about.wxss**/
.container{
    background: #f4f4f4;
    padding: 0;
}
.header{
    position: relative;
    width: 100%;
    height: 400rpx;
    text-align: center;
    box-sizing: border-box;
    overflow: hidden;
}
.header text{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    z-index: 3;
    font-size: 50pt;
    line-height: 400rpx;
    white-space: 2px;
    text-align: center;
    color: #fff;
}
.black-cover{
    position: absolute;
    z-index: 2;
    top:0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #888;
    opacity: .15;
}
.logo{
    position: absolute;
    top:0;
    left: 0;
    z-index: 1;
    width: 100%;
    height: 750rpx;
    border-radius: 10rpx;
    margin-top: -175rpx;
    filter: blur(15px);
}
.content{
    flex: 1;
}
.version{
    height: 104rpx;
    background: #fff;
    display: flex;
    justify-content: space-between;
    padding: 0 55rpx;
    line-height: 104rpx;
    box-sizing: border-box;
    font-size: 12pt;
    color: #666;
    border-bottom: 1px #e5e5e5 solid;
}
.navigator{
    color: #339c74;
}
.describe{
    width: 100%;
    box-sizing: border-box;
    padding: 10rpx 55rpx 20rpx;
    display: flex;
    flex-flow: column nowrap;
    color: #545454;
    line-height: 2;
    background: #fff;
    margin-top: 12rpx;
    border-top: 1px #e5e5e5 solid;
    border-bottom: 1px #e5e5e5 solid;
}
.desc-title{
    font-size: 12pt;
    line-height: 2.5;
}
.desc-content{
    padding-bottom: 15rpx;
}
.footer{
    font-size: 10pt;
    text-align: center;
    padding: 40rpx 0 20rpx;
    color: #ccc;
}
.footer-lanshan{
    height: 10pt;
    line-height: 10pt;
    display: flex;
    align-items: stretch;
    justify-content: center;
    margin-bottom: 5rpx;
}
.footer-lanshan image{
    width: 10pt;
    height: 10pt;
}
